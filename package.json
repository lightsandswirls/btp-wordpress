{
  "name": "wp-lightsandswirls",
  "version": "1.0.0",
  "description": "Webpack setup for WordPress themes by lightsandswirls",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --progress --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dashboard -- webpack --mode development --watch --progress --config webpack.config.dev.js",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lightsandswirls/webpack-lightsandswirls.git"
  },
  "author": "lightsandswirls",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lightsandswirls/webpack-lightsandswirls/issues"
  },
  "homepage": "https://github.com/lightsandswirls/webpack-lightsandswirls#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^10.1.2",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dashboard": "^3.3.8"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3"
  }
}
